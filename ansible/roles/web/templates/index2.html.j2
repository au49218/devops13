<html>
<head>
<style>
table, th, td {
  border: 1px solid black;
}
th, td {
  padding: 5px;
}
</style>
</head>
<body>

<table>
  <tr>
    <th>Key</th>
    <th>Val</th>
  </tr>
{% for key, val in index_key_val.items() %}
  <tr>
    <td>{{ key }}</td>
    <td>{{ val }}</td>
  </tr>
{% endfor %}
</table>
</body>
</html>
